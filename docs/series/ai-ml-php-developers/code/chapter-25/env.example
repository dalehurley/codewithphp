APP_NAME=SmartDash
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://localhost:8000

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
APP_MAINTENANCE_STORE=database

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=smartdash
DB_USERNAME=root
DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@smartdash.local"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# AI Service Configuration
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MODEL=gpt-4-turbo-preview
OPENAI_MAX_TOKENS=500

GOOGLE_CLOUD_VISION_KEY=your-google-vision-api-key-here
VISION_PROVIDER=cloud

# AI Service Settings
AI_CACHE_ENABLED=true
AI_CACHE_TTL=3600
AI_REQUEST_TIMEOUT=30

# Chatbot Settings
CHATBOT_MAX_CONTEXT_MESSAGES=10
CHATBOT_SYSTEM_PROMPT="You are a helpful customer support assistant for SmartDash, an AI-powered analytics platform."

# Recommendation Settings
RECOMMENDER_SIMILARITY_THRESHOLD=0.3
RECOMMENDER_MAX_RECOMMENDATIONS=10

# Forecast Settings
FORECAST_MOVING_AVERAGE_WINDOW=7
FORECAST_DEFAULT_HORIZON=30

# Vision Settings
VISION_CONFIDENCE_THRESHOLD=0.7
VISION_MAX_TAGS=10

